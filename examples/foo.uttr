id x = x

fail() | false = nil

fb x
	| x % 15 == 0 = "fizzbuzz"
	| x % 3 == 0  = "fizz"
	| x % 5 == 0  = "buzz"
	| x           = str(x)

range(10)

map(id, range(10))

map(fb, range(100))


