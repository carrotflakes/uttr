if(cond, then, else) | cond = then | true = else

if(true, "true!", "false!")
if(false, "true!", "false!")
if(null, "true!", "false!")

map(fun, x:xs) = fun(x):map(fun, xs),
   (_, []) = []

foldl
	(fun, l, []) = l,
	(fun, l, r:rs) = foldl(fun, fun(l, r), rs)

reverse_
	([], ys) = ys,
	(x:xs, ys) = reverse_(xs, x:ys)
reverse(l) = reverse_(l, [])

